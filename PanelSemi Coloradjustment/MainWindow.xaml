<mah:MetroWindow
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PanelSemi_Coloradjustment"
        xmlns:CtLib ="clr-namespace:CtLib.Library.Wpf;assembly=CtLib.Library.Wpf"
        xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls" x:Class="PanelSemi_Coloradjustment.MainWindow"
        mc:Ignorable="d"
        
        Title="{Binding PanelSemi_SplicingVersion}" Height="900" Width="1100" Background="{x:Null}" WindowStartupLocation="CenterScreen" TitleCharacterCasing="Normal">
    <mah:MetroWindow.TitleTemplate>
        <DataTemplate>
            <StackPanel Orientation="Horizontal"
			HorizontalAlignment="Left"
			VerticalAlignment="Center"
			Margin="5,0,0,0">
                <Image Source="/Panelsemi white.png" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Stretch="Uniform" Margin="0,2,5,2" />
                <Border Margin="2,2,0,2"
	                CornerRadius="3"
	                Background="#3C3C3C">
                    <TextBlock Padding="5,2.5,5,2.5" Text="Color adjustment App" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                </Border>
                <Border Margin="10,2,0,2"
				CornerRadius="3"
				Background="#3C3C3C">
                    <TextBlock Padding="5,2.5,5,2.5" Text="{TemplateBinding Content}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                </Border>
            </StackPanel>
        </DataTemplate>
    </mah:MetroWindow.TitleTemplate>
    <Window.DataContext>
        <local:MainProcess/>
    </Window.DataContext>

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition Height="0.1*"/>
            <RowDefinition Height="1.3*"/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
           
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>

        </Grid.ColumnDefinitions>

        <Border Grid.Column="1" Grid.RowSpan="2" Grid.ColumnSpan="1">
            <StackPanel Margin="2">
                <TextBlock Text="色域選擇" Foreground="#FFFEFEFE" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="20"/>
                <Separator/>
                <Border Height="30">
                    <StackPanel Orientation="Horizontal" >
                        <TextBlock Text="1020階" Foreground="#FFF8F8F8" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14"/>
                        <Rectangle Width="2" Fill="White" Margin="4,0,4,0"/>
                        <CheckBox IsChecked="{Binding ColorSpaceCheckBoxStates[0]}" Content="R" Margin="2,0,0,0" FontSize="18"/>
                        <CheckBox IsChecked="{Binding ColorSpaceCheckBoxStates[1]}" Content="G" Margin="2,0,0,0" FontSize="18"/>
                        <CheckBox IsChecked="{Binding ColorSpaceCheckBoxStates[2]}" Content="B" Margin="2,0,0,0" FontSize="18"/>
                        <CheckBox IsChecked="{Binding ColorSpaceCheckBoxStates[3]}" Content="W" Margin="2,0,0,0" FontSize="18"/>
                    </StackPanel>
                </Border>
                <Separator/>
                <Border Height="30">
                    <StackPanel Orientation="Horizontal" >
                        <TextBlock Text="0512階" Foreground="#FFF8F8F8" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14"/>
                        <Rectangle Width="2" Fill="White" Margin="4,0,4,0"/>
                        <CheckBox IsChecked="{Binding ColorSpaceCheckBoxStates[4]}" Content="R" Margin="2,0,0,0" FontSize="18"/>
                        <CheckBox IsChecked="{Binding ColorSpaceCheckBoxStates[5]}" Content="G" Margin="2,0,0,0" FontSize="18"/>
                        <CheckBox IsChecked="{Binding ColorSpaceCheckBoxStates[6]}" Content="B" Margin="2,0,0,0" FontSize="18"/>
                        <CheckBox IsChecked="{Binding ColorSpaceCheckBoxStates[7]}" Content="W" Margin="2,0,0,0" FontSize="18"/>
                    </StackPanel>
                </Border>
                <Separator/>
                <Border Height="30">
                    <StackPanel Orientation="Horizontal" >
                        <TextBlock Text="0256階" Foreground="#FFF8F8F8" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14"/>
                        <Rectangle Width="2" Fill="White" Margin="4,0,4,0"/>
                        <CheckBox IsChecked="{Binding ColorSpaceCheckBoxStates[8]}" Content="R" Margin="2,0,0,0" FontSize="18"/>
                        <CheckBox IsChecked="{Binding ColorSpaceCheckBoxStates[9]}" Content="G" Margin="2,0,0,0" FontSize="18" />
                        <CheckBox IsChecked="{Binding ColorSpaceCheckBoxStates[10]}" Content="B" Margin="2,0,0,0" FontSize="18"/>
                        <CheckBox IsChecked="{Binding ColorSpaceCheckBoxStates[11]}" Content="W" Margin="2,0,0,0" FontSize="18"/>
                    </StackPanel>
                </Border>
                <Separator/>
                <Border Height="30">
                    <StackPanel Orientation="Horizontal" >
                        <TextBlock Text="0128階" Foreground="#FFF8F8F8" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="14"/>
                        <Rectangle Width="2" Fill="White" Margin="4,0,4,0"/>
                        <CheckBox IsChecked="{Binding ColorSpaceCheckBoxStates[12]}" Content="R" Margin="2,0,0,0" FontSize="18"/>
                        <CheckBox IsChecked="{Binding ColorSpaceCheckBoxStates[13]}" Content="G" Margin="2,0,0,0" FontSize="18"/>
                        <CheckBox IsChecked="{Binding ColorSpaceCheckBoxStates[14]}" Content="B" Margin="2,0,0,0" FontSize="18"/>
                        <CheckBox IsChecked="{Binding ColorSpaceCheckBoxStates[15]}" Content="W" Margin="2,0,0,0" FontSize="18"/>
                    </StackPanel>
                </Border>
                <Separator/>
               

               
                
                
            </StackPanel>
        </Border>
        <Border Margin="2"  Grid.Column="0" Grid.ColumnSpan="1" Grid.Row="5" Grid.RowSpan="1">
            <Grid>
                <StackPanel VerticalAlignment="Bottom">
                    <Button Command="{Binding StartandInit}" Content="{Binding EnterOrExistColorMoode}" Margin="0,5,0,0" FontSize="16" Background="#FF207900" Height="50"/>
                </StackPanel>
            </Grid>
           
        </Border>
        <Border Grid.Column="0" Grid.ColumnSpan="1" Grid.RowSpan="5">
            <StackPanel Margin="2">
                <TextBlock Text="系統" Foreground="#FFFEFEFE" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="20"/>
                <Separator/>
                <Button Command="{Binding USB_ComPort_FindandOpen}" Content="連線" Margin="0,5,0,0" FontSize="16" />
                <ItemsControl ItemsSource="{Binding Panel_ID_CheckBoxes}" >
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>
                            <CheckBox Content="{Binding Content}" IsChecked="{Binding IsChecked}"  FontSize="18" Margin="5"/>
                        </DataTemplate>
                    </ItemsControl.ItemTemplate>
                </ItemsControl>
                
            </StackPanel>

        </Border>
        <Border Margin="2" Grid.Column="2" Grid.RowSpan="2" Grid.ColumnSpan="2" BorderThickness="2" BorderBrush="White">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="0.5*"/>
                    <RowDefinition Height="1*"/>
                    <RowDefinition Height="1*"/>
                    <RowDefinition Height="1*"/>
                    <RowDefinition Height="1*"/>
                </Grid.RowDefinitions>
                <Border  Grid.ColumnSpan="8" BorderBrush="White" BorderThickness="1">
                    <StackPanel Orientation="Horizontal">
                        <Image Source="/Panelsemi white.png" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Stretch="Fill" Margin="5,2,5,2" />
                    </StackPanel>
                </Border>
                <Rectangle Fill="{Binding List_fillBrush[0]}" Grid.Column="0" Grid.Row="1"/>
                <Rectangle Fill="{Binding FillBrushX1T2}" Grid.Column="0" Grid.Row="2"/>
                <Rectangle Fill="{Binding FillBrushX1T3}" Grid.Column="0" Grid.Row="3"/>
                <Rectangle Fill="{Binding FillBrushX1T4}" Grid.Column="0" Grid.Row="4"/>

                <Rectangle Fill="{Binding FillBrushX2T1}" Grid.Column="1" Grid.Row="1"/>
                <Rectangle Fill="{Binding FillBrushX2T2}" Grid.Column="1" Grid.Row="2"/>
                <Rectangle Fill="{Binding FillBrushX2T3}" Grid.Column="1" Grid.Row="3"/>
                <Rectangle Fill="{Binding FillBrushX2T4}" Grid.Column="1" Grid.Row="4"/>

                <Rectangle Fill="{Binding FillBrushX3T1}" Grid.Column="2" Grid.Row="1"/>
                <Rectangle Fill="{Binding FillBrushX3T2}" Grid.Column="2" Grid.Row="2"/>
                <Rectangle Fill="{Binding FillBrushX3T3}" Grid.Column="2" Grid.Row="3"/>
                <Rectangle Fill="{Binding FillBrushX3T4}" Grid.Column="2" Grid.Row="4"/>

                <Rectangle Fill="{Binding FillBrushX4T1}" Grid.Column="3" Grid.Row="1"/>
                <Rectangle Fill="{Binding FillBrushX4T2}" Grid.Column="3" Grid.Row="2"/>
                <Rectangle Fill="{Binding FillBrushX4T3}" Grid.Column="3" Grid.Row="3"/>
                <Rectangle Fill="{Binding FillBrushX4T4}" Grid.Column="3" Grid.Row="4"/>

                <Rectangle Fill="{Binding AFillBrushX1T1}" Grid.Column="4" Grid.Row="1"/>
                <Rectangle Fill="{Binding AFillBrushX1T2}" Grid.Column="4" Grid.Row="2"/>
                <Rectangle Fill="{Binding AFillBrushX1T3}" Grid.Column="4" Grid.Row="3"/>
                <Rectangle Fill="{Binding AFillBrushX1T4}" Grid.Column="4" Grid.Row="4"/>

                <Rectangle Fill="{Binding AFillBrushX2T1}" Grid.Column="5" Grid.Row="1"/>
                <Rectangle Fill="{Binding AFillBrushX2T2}" Grid.Column="5" Grid.Row="2"/>
                <Rectangle Fill="{Binding AFillBrushX2T3}" Grid.Column="5" Grid.Row="3"/>
                <Rectangle Fill="{Binding AFillBrushX2T4}" Grid.Column="5" Grid.Row="4"/>

                <Rectangle Fill="{Binding AFillBrushX3T1}" Grid.Column="6" Grid.Row="1"/>
                <Rectangle Fill="{Binding AFillBrushX3T2}" Grid.Column="6" Grid.Row="2"/>
                <Rectangle Fill="{Binding AFillBrushX3T3}" Grid.Column="6" Grid.Row="3"/>
                <Rectangle Fill="{Binding AFillBrushX3T4}" Grid.Column="6" Grid.Row="4"/>

                <Rectangle Fill="{Binding AFillBrushX4T1}" Grid.Column="7" Grid.Row="1"/>
                <Rectangle Fill="{Binding AFillBrushX4T2}" Grid.Column="7" Grid.Row="2"/>
                <Rectangle Fill="{Binding AFillBrushX4T3}" Grid.Column="7" Grid.Row="3"/>
                <Rectangle Fill="{Binding AFillBrushX4T4}" Grid.Column="7" Grid.Row="4"/>

            </Grid>

        </Border>
        <Border Grid.Column="4" Grid.RowSpan="2" Grid.ColumnSpan="1">
            <StackPanel Margin="2">
                <TextBlock Text="調整模式" Foreground="#FFFEFEFE" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="20"/>
                <Separator/>
                <ComboBox ItemsSource="{Binding Mode_Item}" SelectedItem="{Binding Mode_SelectedItem}"  Margin="5"/>
                <Separator/>
                <TextBlock Text="XB Chose" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="18"/>
                <Border Height="30" IsEnabled="{Binding Is1x4}">
                    <StackPanel Orientation="Horizontal">
                        <CheckBox IsChecked="{Binding Ix4checkBoxStates[0]}" Content="1" Margin="5,0,0,0" FontSize="18"/>
                        <CheckBox IsChecked="{Binding Ix4checkBoxStates[1]}" Content="2" Margin="5,0,0,0" FontSize="18"/>
                        <CheckBox IsChecked="{Binding Ix4checkBoxStates[2]}" Content="3" Margin="5,0,0,0" FontSize="18"/>
                        <CheckBox IsChecked="{Binding Ix4checkBoxStates[3]}" Content="4" Margin="5,0,0,0" FontSize="18"/>
                    </StackPanel>

                </Border>
                <Border Height="30" IsEnabled="{Binding Is1x4}">
                    <StackPanel Orientation="Horizontal">
                        <CheckBox IsChecked="{Binding Ix4checkBoxStates[4]}" Content="5" Margin="5,0,0,0" FontSize="18"/>
                        <CheckBox IsChecked="{Binding Ix4checkBoxStates[5]}" Content="6" Margin="5,0,0,0" FontSize="18"/>
                        <CheckBox IsChecked="{Binding Ix4checkBoxStates[6]}" Content="7" Margin="5,0,0,0" FontSize="18"/>
                        <CheckBox IsChecked="{Binding Ix4checkBoxStates[7]}" Content="8" Margin="5,0,0,0" FontSize="18"/>
                    </StackPanel>
                </Border>
                <Separator/>
                <TextBlock Text="Tile Chose" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="18"/>
                <Border Height="30" IsEnabled="{Binding IsSingle}">
                    <StackPanel Orientation="Horizontal">
                        <CheckBox IsChecked="{Binding SinglecheckBoxStates[0]}" Content="1" Margin="5,0,0,0" FontSize="18"/>
                        <CheckBox IsChecked="{Binding SinglecheckBoxStates[1]}" Content="2" Margin="5,0,0,0" FontSize="18"/>
                        <CheckBox IsChecked="{Binding SinglecheckBoxStates[2]}" Content="3" Margin="5,0,0,0" FontSize="18"/>
                        <CheckBox IsChecked="{Binding SinglecheckBoxStates[3]}" Content="4" Margin="5,0,0,0" FontSize="18"/>
                    </StackPanel>
                </Border>
            </StackPanel>
        </Border>
        <Separator Grid.Column="1" Grid.ColumnSpan="4"  Grid.Row="2"/>
        <Border Margin="2" Grid.Column="1" Grid.ColumnSpan="1" Grid.Row="3" Grid.RowSpan="1" BorderThickness="1" BorderBrush="Red" Background="#FF915858">
            <Grid Width="208">
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition Height="2*"/>
                </Grid.RowDefinitions>
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" >
                    <TextBlock Text="R" FontSize="30" Foreground="#FFFCFCFC" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="8"/>
                </StackPanel>


                <StackPanel Grid.Row="1" Margin="5,0,5,0" Orientation="Horizontal">
                    <TextBox Margin="5,5,5,5" Text="{Binding ValueR}" FontSize="26" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="70" VerticalAlignment="Center" Height="77" Background="#FF252525"/>
                    <Grid Margin="0,5,0,5">
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <Button Grid.Row="0" Command="{Binding AdjValueR}" CommandParameter="up" Width="110" Height="35" Margin="5,0,5,5" VerticalAlignment="Bottom">
                            <StackPanel>
                                <Image Source="/White Up.png" Stretch="Fill" Width="60" Height="25"/>
                            </StackPanel>
                        </Button>
                        <Button Grid.Row="1" Command="{Binding AdjValueR}" CommandParameter="dwn" Width="110" Height="35" Margin="5,5,5,0" VerticalAlignment="Top">
                            <StackPanel>
                                <Image Source="/White dwn.png" Stretch="Fill" Height="25" Width="60"/>
                            </StackPanel>
                        </Button>
                    </Grid>
                </StackPanel>
            </Grid>
        </Border>
        <Border Margin="2" Grid.Column="2" Grid.ColumnSpan="1" Grid.Row="3" Grid.RowSpan="1" BorderThickness="1" BorderBrush="Lime" Background="#FF58913C" >
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition Height="2*"/>
                </Grid.RowDefinitions>
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" >
                    <TextBlock Text="G" FontSize="30" Foreground="#FFFCFCFC" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="8"/>

                </StackPanel>


                <StackPanel Grid.Row="1" Margin="5,0,5,0" Orientation="Horizontal">
                    <TextBox Margin="5"  Text="{Binding ValueG}" FontSize="26" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="70" VerticalAlignment="Center" Height="77"/>
                    <Grid Margin="0,5,0,5">
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <Button Width="110" Command="{Binding AdjValueG}" CommandParameter="up"  Height="35" Margin="5,0,5,5" VerticalAlignment="Bottom">
                            <StackPanel>
                                <Image Source="/White Up.png" Stretch="Fill" Width="60" Height="25"/>
                            </StackPanel>
                        </Button>
                        <Button Width="110" Grid.Row="1" Command="{Binding AdjValueG}" CommandParameter="dwn" Height="35" Margin="5,5,5,0" VerticalAlignment="Top">
                            <StackPanel>
                                <Image Source="/White dwn.png" Stretch="Fill" Height="25" Width="60"/>
                            </StackPanel>
                        </Button>
                    </Grid>
                </StackPanel>
            </Grid>
        </Border>
        <Border Margin="2" Grid.Column="3" Grid.ColumnSpan="1" Grid.Row="3" Grid.RowSpan="1" BorderThickness="1" BorderBrush="Blue" Background="#FF3C5891" >
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition Height="2*"/>
                </Grid.RowDefinitions>
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" >
                    <TextBlock Text="B" FontSize="30" Foreground="#FFFCFCFC" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="8"/>
                </StackPanel>

                <StackPanel Grid.Row="1" Margin="5,0,5,0" Orientation="Horizontal">
                    <TextBox Margin="5"   Text="{Binding ValueB}" FontSize="26" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="70" VerticalAlignment="Center" Height="77"/>
                    <Grid Margin="0,5,0,5">
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <Button Width="110" Command="{Binding AdjValueB}" CommandParameter="up"  Height="35" Margin="5,0,5,5" VerticalAlignment="Bottom">
                            <StackPanel>
                                <Image Source="/White Up.png" Stretch="Fill" Width="60" Height="25" VerticalAlignment="Center"/>
                            </StackPanel>
                        </Button>
                        <Button Width="110" Grid.Row="1" Command="{Binding AdjValueB}" CommandParameter="dwn" Height="35" Margin="5,5,5,0" VerticalAlignment="Top">
                            <StackPanel>
                                <Image Source="/White dwn.png" Stretch="Fill" Height="25" Width="60"/>
                            </StackPanel>
                        </Button>
                    </Grid>

                </StackPanel>
            </Grid>

        </Border>
        
        <Border Margin="2" Grid.Column="4" Grid.ColumnSpan="1" Grid.Row="3" Grid.RowSpan="1">
            <StackPanel>
                <Button Command="{Binding RecoverDefaultValue}"  Content="恢復為預設色差" Margin="0,5,0,0" FontSize="16" Background="{x:Null}"/>
            </StackPanel>
        </Border>
        <Border Margin="2" Grid.Column="4" Grid.ColumnSpan="1" Grid.Row="5" Grid.RowSpan="1">
            <StackPanel VerticalAlignment="Bottom">
                <Button Command="{Binding SaveColorInfo}" Content="儲存色差" Margin="0,5,0,0" FontSize="16" Background="#FFA82507" Height="50"/>
            </StackPanel>
        </Border>
