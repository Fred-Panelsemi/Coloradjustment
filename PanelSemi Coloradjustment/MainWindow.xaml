                <TextBlock Text="XB3" Grid.Column="2"  Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                <TextBlock Text="XB4" Grid.Column="3"  Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                
